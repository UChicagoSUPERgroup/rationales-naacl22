{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-empty-interface": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "curly": false,
    "prefer-const": false
  },
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  }
}
